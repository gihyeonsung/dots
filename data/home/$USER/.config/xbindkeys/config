"pulsemixer --change-volume +5"
  XF86AudioRaiseVolume

"pulsemixer --change-volume -5"
  XF86AudioLowerVolume

"pulsemixer --toggle-mute"
  XF86AudioMute

"backlit dec 15"
  XF86MonBrightnessDown

"backlit inc 15"
  XF86MonBrightnessUp

"scrot --quality 100 --silent '%Y-%m-%d-%H-%M-%S_$wx$b.png' --exec 'mv $f ~/images/screenshots'"
  Print
